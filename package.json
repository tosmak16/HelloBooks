{
    "name": "server",
    "version": "1.0.0",
    "description": "",
    "main": "app.js",
    "scripts": {
        "lint": "markdownlint --config linters/.markdownlint.json",
        "test": "babel-watch ./node_modules/mocha/bin/_mocha server/test/test.js --timeout 10000  --presets es2015,stage-2",
        "build": "babel ./server/server --out-dir build",
        "start:prod": "NODE_ENV=production&& ./node_modules/.bin/babel-node ./server/server.js",
        "start:development": "babel-watch ./config/server --presets es2015,stage-2",
        "server": "nodemon --watch server --exec babel-node -- ./config/server.js",
        "start": "npm run db:drop && npm run migrate:db && npm run db:seed && npm run heroku ",
        "migrate:db": "sequelize db:migrate",
        "db:seed": "sequelize db:seed:all",
        "db:drop": "sequelize db:migrate:undo:all",
        "migrate:test:db": "sequelize db:migrate",
        "heroku": " babel-node ./config/server --exec babel-node --presets es2015"
    },
    "keywords": [
        "style guide",
        "lint",
        "airbnb",
        "es6"
