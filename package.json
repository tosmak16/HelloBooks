{
  "name": "server",
  "version": "1.0.0",
  "description": "",
  "main": "app.js",
  "scripts": {
    "heroku-postbuild": "NODE_ENV=production  npm run db:drop && npm run migrate:db && npm run db:seed && npm run build",
    "build": "NODE_ENV=production webpack --config webpack.prod.js --progress",
    "lint": "markdownlint --config linters/.markdownlint.json",
    "test": "NODE_ENV=test npm run db:drop && NODE_ENV=test npm run migrate:db && NODE_ENV=test nyc --reporter=html --reporter=text babel-node ./node_modules/mocha/bin/_mocha server/test/**.js --presets es2015,stage-2",
    "tests": "jest",
    "test:watch": "npm test ./client/__tests__/ -- --watch",
    "start": "NODE_ENV=production babel-node ./app.js ",
    "start:development": "babel-watch ./config/server --presets es2015,stage-2",
    "server": "nodemon --watch server --exec babel-node -- ./app.js",
    "start:dev": "NODE_ENV=development babel-node ./app.js ",
    "migrate:db": "sequelize db:migrate",
    "db:seed": "sequelize db:seed:all",
    "db:drop": "sequelize db:migrate:undo:all",
    "migrate:test:db": "sequelize db:migrate",
    "heroku": "babel-node ./app.js",
    "coveralls": "nyc report --reporter=text-lcov | node node_modules/.bin/coveralls"
  },
